<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro"  name="robot">

    <material name="orange">
        <color rgba="1 0.3 0.1 1"/>
    </material>

    <material name="blue">
        <color rgba="0.2 0.2 1 1"/>
    </material>

    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>


	<link name="stand_link">
		<visual>
		    <origin xyz="0 0 0.025" rpy="0 0 0"/>
			<geometry>
				<box size="0.5 0.5 0.05"/>
			</geometry>
			<material name="black"/>
		</visual>
  		<collision>
  			<origin xyz="0 0 0.1" rpy="0 0 0"/>
			<geometry>
				<box size="0.5 0.5 0.05"/>
			</geometry>
  		</collision>
  		<inertial>
    		<mass value="141"/>
      		<inertia ixx="3.8" ixy="0" ixz="0" iyy="3.8" iyz="0" izz="18"/>
 		 </inertial>
	</link>


    <link name="base_link">
		<visual>
		    <origin xyz="0 0 0.2" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.01" length="0.4"/>
			</geometry>
			<material name="blue"/>
		</visual>
  		<collision>
  			<origin xyz="0 0 0.2" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.01" length="0.4"/>
			</geometry>
  		</collision>
		<inertial>
    		<mass value="0.4536"/>
      		<inertia ixx="0.2" ixy="0" ixz="0" iyy="0.2" iyz="0" izz="0.8"/>
 		 </inertial>
	</link>
	
<link name="arm1_link">
		<visual>
			<origin xyz="0.078 0 0" rpy="0 1.57079632679 0"/>
			<geometry>
				<box size="0.003 0.036 0.156"/>
			</geometry>
	     	<material name="blue"/>
	  	 </visual>
		<collision>
          <origin xyz="0.078 0 0" rpy="0 1.57079632679 0"/>
			<geometry>
				<box size="0.003 0.036 0.156"/>
			</geometry>
       </collision>
	   <inertial>
    		<mass value="0.4536"/>
      		<inertia ixx="0.002" ixy="0" ixz="0" iyy="0.000215" iyz="0" izz="0.0008"/>
 		 </inertial>
	</link>

<link name="arm2_link">
		<visual>
			<origin xyz="0 0 -0.0675" rpy="0 0 0"/>
			<geometry>
				<box size="0.003 0.036 0.135"/>
			</geometry>
	     	<material name="blue"/>
	   </visual>
	   <collision>
         <origin xyz="0 0 -0.0675" rpy="0 0 0"/>
			<geometry>
				<box size="0.003 0.036 0.135"/>
			</geometry>
       </collision>
	 	  <inertial>
     		 <mass value="0.40824"/>
     		 <inertia ixx="0.0015" ixy="0" ixz="0" iyy="0.0002" iyz="0" izz="0.0007"/>
  	  </inertial>
	</link>

	<joint name="stand_joint" type="fixed">
    	<parent link="stand_link"/>
    	<child link="base_link"/>
    	<origin xyz="0 0 0" rpy="0 0 0"/>
	</joint>

	<joint name="arm1_joint" type="continuous">
    	<parent link="base_link"/>
    	<child link="arm1_link"/>
    	<origin xyz="0 0 0.4" rpy="0 0 0"/>
  		<axis xyz = "0 0 1"/>
	</joint>

	<joint name="arm2_joint" type="continuous">
    	<parent link="arm1_link"/>
    	<child link="arm2_link"/>
    	<origin xyz="0.156 0 0" rpy="0 0 0"/>
  		<axis xyz = "1 0 0"/>
	</joint> 
	


</robot>